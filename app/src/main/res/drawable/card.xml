<?xml version="1.0" encoding="utf-8"?>
<layer-list
        xmlns:android="http://schemas.android.com/apk/res/android">

        <item>
            <shape
                android:shape="rectangle">
<gradient
                  android:startColor="#25B0B0B0"
                  android:endColor="#40F5F5F5"
                  android:type="radial"
                  android:gradientRadius="105dp"

                  />
                <corners android:radius = "5dp"/>
            </shape>
        </item>
    <item
            android:right="17dp"
            android:left="17dp"
            android:bottom="17dp"
            android:top="17dp"
    >
        <shape
                android:shape="rectangle">
        <solid  android:color = "@color/front_color"/>
        <corners android:radius = "5dp"/>
        </shape>
    </item>

</layer-list>


